{
  "compilerOptions": {
    "target": "ES2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "jsx": "react",
    "jsxImportSource": "@emotion/react",
    "baseUrl": "./",
    "paths": {
      "rettle/core": [
        "node_modules/rettle/bin/rettle-core"
      ],
      "@/*": ["./src/*"]
    }
  },
  "include": ["./src"],
  "exclude": ["node_modules", ".cache"],
  "extends": "./.cache/tsconfig.json"
}
